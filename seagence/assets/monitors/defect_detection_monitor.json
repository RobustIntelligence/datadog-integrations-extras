{
  "version": 2,
  "created_at": "2023-07-05",
  "last_updated_at": "2023-10-20",
  "title": "Seagence Defect Detection Monitor",
  "tags": [
    "integration:seagence"
  ],
  "description": "Alert when Seagence detects a defect.",
  "definition": {
    "message": "Seagence detected a defect. Visit SeagenceWeb for complete information.",
    "name": "Seagence Defect Detection Monitor",
    "options": {
      "enable_logs_sample": false,
      "include_tags": false,
      "notify_audit": false,
      "on_missing_data": "show_no_data",
      "restriction_query": null,
      "silenced": {},
      "thresholds": {
        "comparison": ">",
        "critical": 0,
        "period": {
          "digit": 5,
          "name": "5 minute average",
          "no_data_timeframe": 10,
          "seconds": 300,
          "tense": "last",
          "text": "5 minutes",
          "timeString": "5m",
          "unit": "minutes",
          "unitAbbreviation": "m",
          "value": "last_5m"
        },
        "timeAggregator": "avg"
      }
    },
    "priority": null,
    "query": "events(\"status:error source:seagence\").rollup(\"count\").last(\"5m\") > 0",
    "restricted_roles": null,
    "tags": [
      "integration:seagence"
    ],
    "type": "event-v2 alert"
  }
}